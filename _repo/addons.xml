<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.program.chameleonbuilds" name="Chameleon Builds" version="1.0.8" provider-name="Chameleon-Builds">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.addon.common" />
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Chameleon-Builds</summary>
    <description lang="en">Easy Way To Install Your Chameleon build!</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="plugin.video.entertainmentlounge" name="Entertainment Lounge" version="0.0.23" provider-name="Chameleon-Builds">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.addon.common" />
	<import addon="script.module.beautifulsoup" version="3.2.1"/>
	<import addon="script.module.beautifulsoup4" />
	<import addon="script.module.httplib2" />
	<import addon="script.module.urlresolver"/>
	<import addon="script.module.requests" />

  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Entertainment Lounge</summary>
    <description lang="en">Entertainment Lounge</description>
    <platform>all</platform>
  </extension>
</addon>

    <addon id="repository.ChameleonEntertainment" name="Chameleon Entertainment Repo" version="1.0.0" provider-name="Chameleon">
